{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "noEmit": true,
    "strict": true ,
    "moduleResolution": "node" ,
    "typeRoots": [ "./node_modules/@types" ] ,
    "allowSyntheticDefaultImports": true,        
    "esModuleInterop": true ,
    "skipLibCheck": true ,
    "forceConsistentCasingInFileNames": true ,
  },
  "exclude": ["node_modules", ".build"]
}
